@model ElectronicsStore.AdminApp.Models.Import.SupplierDeleteRequest

@{
    ViewData["Title"] = "Xóa nhà cung cấp";
}

<div class="container-fluid px-4">
    <h1 class="mt-4">Xóa nhà cung cấp</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Trang chủ</a></li>
        <li class="breadcrumb-item"><a asp-action="GetSupplierPagination" asp-controller="Import">Danh sách nhà cung cấp</a></li>
        <li class="breadcrumb-item active">Xóa nhà cung cấp</li>
    </ol>
    <div class="card mb-4">
        <div class="card-header">
            <a class="btn btn-success" asp-action="GetSupplierPagination" asp-controller="Import">Về danh sách nhà cung cấp</a>
        </div>
        <div class="card-body">
            <div>
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Id)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Id)
                    </dd>
                </dl>

                <form asp-action="DeleteSupplier">
                    <input type="submit" value="Xóa" class="btn btn-danger" />
                </form>
            </div>
        </div>
    </div>
</div>
